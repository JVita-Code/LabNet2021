@model LabNet2021.Tp7.MVC.Models.ShipperView

@{
    ViewBag.Tittle = "Shipper Page";
}

<h1>Vista para Insertar</h1>

@using (Html.BeginForm("Insert", "Shipper", FormMethod.Post))
  {

<div class="form-group">
    <label>CompanyName: </label>
    @Html.TextBoxFor(s => s.CompanyName, new { @class = "form-control" })

    
</div>

<div class="form-group">
    <label>Phone: </label>
    @Html.TextBoxFor(s => s.Phone, new { @class = "form-control" })

    <input class="btn btn-danger" type="submit" value="Guardar" />
</div>


    
  }